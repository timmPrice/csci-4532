#! /usr/local/bin/vvp
:ivl_version "13.0 (devel)" "(s20250103-64-gdad78d525)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x15aac7b0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x15ae45e0 .scope module, "main" "main" 3 1;
 .timescale 0 0;
S_0x15ae4770 .scope module, "tb_myand" "tb_myand" 4 3;
 .timescale -9 -12;
v0x15af7d10_0 .var "a", 0 0;
v0x15af7de0_0 .var "b", 0 0;
v0x15af7eb0_0 .net "out", 0 0, L_0x15af7fb0;  1 drivers
S_0x15ae4900 .scope module, "uut" "myand" 4 8, 3 11 0, S_0x15ae4770;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "x";
    .port_info 1 /INPUT 1 "y";
    .port_info 2 /OUTPUT 1 "z";
L_0x15af7fb0 .functor AND 1, v0x15af7d10_0, v0x15af7de0_0, C4<1>, C4<1>;
v0x15ae5490_0 .net "x", 0 0, v0x15af7d10_0;  1 drivers
v0x15af7b30_0 .net "y", 0 0, v0x15af7de0_0;  1 drivers
v0x15af7bf0_0 .net "z", 0 0, L_0x15af7fb0;  alias, 1 drivers
    .scope S_0x15ae45e0;
T_0 ;
    %vpi_call/w 3 5 "$display", "Hello, World" {0 0 0};
    %delay 276447232, 23283;
    %vpi_call/w 3 6 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x15ae4770;
T_1 ;
    %vpi_call/w 4 15 "$display", "Time | a b | out" {0 0 0};
    %vpi_call/w 4 16 "$monitor", "%4t | %b %b | %b", $time, v0x15af7d10_0, v0x15af7de0_0, v0x15af7eb0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15af7d10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15af7de0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15af7d10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x15af7de0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x15af7d10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15af7de0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x15af7d10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x15af7de0_0, 0, 1;
    %delay 10000, 0;
    %vpi_call/w 4 24 "$finish" {0 0 0};
    %end;
    .thread T_1;
